{
  "name": "react-konami-code",
  "version": "1.4.0",
  "description": "Trigger an easter egg by pressing a sequence of keys.",
  "main": "dist/Konami.js",
  "types": "dist/Konami.d.ts",
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint src/*",
    "test": "jest",
    "test:coverage": "jest --coverage --no-cache && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmarchesin/react-konami-code.git"
  },
  "keywords": [
    "react",
    "konami",
    "konami code",
    "easter egg",
    "code",
    "konamicode",
    "component"
  ],
  "author": "Vinicius Marchesin Araujo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vmarchesin/react-konami-code/issues"
  },
  "homepage": "https://github.com/vmarchesin/react-konami-code#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^22.4.3",
    "path": "^0.12.7",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
